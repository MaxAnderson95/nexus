# Dash0 Website Monitoring Configuration
# Set to 'true' to enable Dash0 telemetry (disabled by default for local dev)
VITE_DASH0_ENABLED=false

# Get your auth token from: https://app.dash0.com/settings/auth-tokens
VITE_DASH0_ENDPOINT_URL=https://ingress.us-east-1.aws.dash0.com
VITE_DASH0_AUTH_TOKEN=your-ingestion-token-here
# Optional: specify dataset (can also be configured via auth token permissions)
VITE_DASH0_DATASET=
VITE_DASH0_ENVIRONMENT=development
